{
    "name": "mastermind/phpenum",
    "description": "Enum helper, that will help you to define Enum types easily by defining just constants.",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Senik Hakobyan",
            "email": "sen.hakobyan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Enum\\": "src/"
        },
        "psr-0": {
            "Enum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
